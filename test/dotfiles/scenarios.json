    },
    "dotfiles-image-node": {
        "image": "mcr.microsoft.com/devcontainers/javascript-node",
        "features": {
            "dotfiles": {
                "DOTFILES_REPO_URL": "git@github.com:jmcejudo/dotfiles.git"
            }
        },
        "remoteUser": "node",
        "containerUser": "node"
    }
}