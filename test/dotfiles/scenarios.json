{
    "dotfiles-image-python": {
        "image": "mcr.microsoft.com/devcontainers/python:3",
        "features": {
            "ghcr.io/devcontainers/features/node:1": {},
            "dotfiles": {
                "DOTFILES_REPO_URL": "git@github.com:jmcejudo/dotfiles.git"
            }
        },
        "remoteUser": "vscode",
