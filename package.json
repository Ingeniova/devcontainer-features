{
  "name": "@ingeniova/devcontainer-features",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "clean": "rm -rf output",
    "clean:all": "pnpm clean && rm -rf node_modules",
    "up:latest": "pnpm up -iL",
    "test": "devcontainer features test --skip-autogenerated",
    "publish:all": "devcontainer features publish -n ingeniova/devcontainer-features src",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "husky": "9.1.7",
    "prettier": "3.7.4",
    "prettier-plugin-sh": "0.18.0",
    "shellcheck": "4.1.0"
  }
}
